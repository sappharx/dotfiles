[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log -l HEAD
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all

[color]
	ui = true

[core]
	excludesfile = ~/.gitignore_global

# allow local customizations in the .gitconfig_local file
[include]
	path = ~/.gitconfig_local
